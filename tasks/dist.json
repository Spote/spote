{
    "paths": {
        "dest": "./dist",
        "js": {
            "vendor": "./dist/vendor",
            "dest": "./dist"
        },
        "css": {
            "vendor": "./dist/vendor",
            "dest": "./dist"
        },
        "images": {
            "dest": "./dist/images"
        },
        "fonts": {
            "dest": "./dist/fonts"
        },
        "jade": {
            "dest": "./dist"
        }
    },
    "files": {
        "js": {
            "vendor": [
                "./bower_components/angular/angular.js",
                "./bower_components/angular-animate/angular-animate.js",
                "./bower_components/angular-ui-router/release/angular-ui-router.js",
                "./bower_components/traceur-runtime/traceur-runtime.js",
                "./bower_components/es6-module-loader/dist/es6-module-loader.js",
                "./bower_components/system.js/dist/system.js",
                "./bower_components/ui-router-extras/release/ct-ui-router-extras.js",
                "./bower_components/angular-logex/dist/log-ex-unobtrusive.js"
            ],
            "src": [
                "./src/**/*.js"
            ]
        },
        "css": {
            "vendor": [
                "./bower_components/animate.css/animate.css"
            ]
        },
        "sass": {
            "watch": [
                "./src/**/*.scss"
            ],
            "src": "./src/app.scss",
            "includes": [
                "./bower_components/open-sans-fontface",
                "./bower_components/Ink/src/sass",
                "./bower_components/Ink/src/sass/contrib"
            ]
        },
        "images": {
            "src": [
                "./bower_components/semantic/build/packaged/images/*"
            ]
        },
        "fonts": {
            "src": [
                "./bower_components/Ink/dist/fonts/**",
                "!./bower_components/Ink/dist/fonts/FontAwesome.*",
                "./bower_components/font-awesome/fonts/**"
            ]
        },
        "jade": {
            "src": [
                "./src/**/*.jade"
            ]
        }
    }
}
